{
    "name": "michaelsochurek/pickup-points",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [ "app/AppKernel.php" ]
    },
    "authors": [
        {
            "name": "Michael Sochurek"
        }
    ],
    "require": {
        "php": ">= 8.3",
        "ext-pdo": "*",
        "symfony/console": "^7.3",
        "psr/log": "^3.0",
        "symfony/config": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/yaml": "^7.3",
        "robmorgan/phinx": "^0.16.10",
        "ext-xmlreader": "*",
        "ext-simplexml": "*",
        "ext-iconv": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13",
        "phpunit/phpunit": "^12.3"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=PSR12 --colors -p src/",
        "phpcbf": "vendor/bin/phpcbf --standard=PSR12 --colors -p src/",
        "test": "phpunit --display-warnings"
    }
}
